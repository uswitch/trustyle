  "funnelProgress": {
    "base": {
      "display": "flex",
      "flexFlow": "row",
      "userSelect": "none",

      "phase": {
        "base": {
          "flex": "auto",
          "flexGrow": 0,
          "marginLeft": "sm",
          "backgroundColor": "white",
          "borderRadius": "999px",
          "whiteSpace": "nowrap",
          "position": "relative",
          "transition": "flex-grow 0.4s",
          "&:first-child": {
            "marginLeft": "0px"
          }
        },
        "variants": {
          "open": {
            "variant": "funnelProgress.base.phase.base",
            "flexGrow": 1,
            "overflow": "hidden"
          }
        }
      },

      "phaseLabel": {
        "base": {
          "position": "relative",
          "whiteSpace": "nowrap",
          "overflow": "hidden",
          "textOverflow": "ellipsis"
        }
      },

      "phaseLabelPart": {
        "base": {
