  "----------------------------- Base Styles": "-----------------------------",
  "styles": {
    "heading": {
      "fontFamily": "heading",
      "color": "grey-90",
      "lineHeight": "heading",
      "fontWeight": "heading",
      "m": 0
    },
    "h1": {
      "variant": "styles.heading",
      "fontSize": ["xxl", "xxxl"],
      "my": "md"
    },
    "h2": {
      "variant": "styles.heading",
      "fontSize": ["xl", "xxl"],
      "my": "md"
    },
    "h3": {
      "variant": "styles.heading",
      "fontSize": ["lg", "xl"],
      "my": "sm"
    },
    "h4": {
      "variant": "styles.heading",
      "fontSize": ["md", "lg"],
      "my": "sm"
    },
    "h5": {
      "variant": "styles.heading",
      "fontSize": ["sm", "md"],
      "my": "sm"
    },
    "h6": {
