{
  "name": "Bankrate",
  "useLocalStorage": false,
  "breakpoints": ["768px", "990px"],
  "zIndices": {
    "min": 0,
    "max": 100
  },
  "shadows": {
    "box": "2px 2px 25px #0004",
    "linkInset": "inset 0 0 0 1px #008fe9",
    "card": "rgba(0, 0, 72, 0.28) 0px 23px 28px -16px, rgba(0, 0, 72, 0.06) 0px -2px 11px 0px",
    "formInput": "inset 0 2px 5px 0 rgba(0, 0, 0, 0.1)",
    "focusInput": "inset 0 0 0 1px #141424",
    "errorInput": "inset 0 0 0 1px #DB1818"
  },
  "fonts": {
    "base": "Roboto, -apple-system, system-ui, system-ui, Arial, sans-serif",
    "body": "Roboto, -apple-system, system-ui, system-ui, Arial, sans-serif",
    "heading": "sofia-pro, -apple-system, sans-serif",
    "monospace": "Menlo, monospace",
    "link": "Roboto, -apple-system, system-ui, system-ui, Arial, sans-serif"
  },
  "lineHeights": {
    "base": "1.5",
    "heading": "1.3"
  },
  "fontSizes": {
    "base": 16,
    "xxs": 12,
    "xs": 14,
    "sm": 16,
    "md": 18,
    "lg": 20,
    "xl": 24,
    "xxl": 30,
    "xxxl": 36
  },
  "fontWeights": {
    "base": 400,
    "light": 300,
    "bold": 600,
    "heading": 500
  },
  "space": {
    "gutter": 16,
    "base": 16,
    "xxs": 4,
    "xs": 8,
    "sm": 16,
    "md": 24,
    "lg": 32,
    "xl": 48,
    "xxl": 64,
    "xxxl": 96
  },
  "sizes": {
    "hero": {
      "width": 1024
    },
    "label": {
      "width": 280
    },
    "input": {
      "height": 58
    },
    "button": {
      "width": 264
    },
    "footer": {
      "width": 576
    }
  },
  "useCustomProperties": false,
  "useBodyStyles": false,
  "colors": {
    "brand": "#0157FF",
    "text": "#4c4c4c",
    "grey-0": "#FFFFFF",
    "grey-05": "#F7F7F7",
    "grey-10": "#F2F2F7",
    "grey-20": "#DFDFE1",
    "grey-30": "#C2C4C8",
    "grey-40": "#ADAFB3",
    "grey-50": "#A0A2A6",
    "grey-60": "#6D6F73",
    "grey-70": "#47494F",
    "grey-80": "#24262A",
    "grey-90": "#141424",
    "grey-100": "#050505",
    "primary": "#0157FF",
    "primary--dark": "rgb(0, 70, 212);",
    "secondary": "#924A8B",
    "secondary--dark": "#f9f9f9",
    "frozen-bg": "#F6F6F6",
    "battleshipGrey": "#65717B",
    "veryLightGrey": "#E9EBED",
    "tomato": "#DB1818",
    "experimental-text": "#29335C",
    "button-secondary": "#FFFFFF",
    "button-secondary--hover": "#f0f1f3",
    "dark-grey-03": "rgba(104,104,135,0.3)",
    "light-grey": "#F2F2F6",
    "light-grey-content": "#202020",
    "border-light-grey": "#E9E9EC",
    "dark-blue": "#001C53",
    "light-blue": "#E5EEFF",
    "dark-grey-blue": "#3E547D",
    "light-grey-blue": "#E8E8F6"
  },
  "radii": {
    "sm": 3,
    "md": 6,
    "lg": 16,
    "xl": 30
  },
  "srOnly": {
    "position": "absolute",
    "width": 1,
    "height": 1,
    "p": 0,
    "m": -1,
    "overflow": "hidden",
    "clip": "rect(0, 0, 0, 0)",
    "border": 0
  },
  "----------------------------- Base Styles": "-----------------------------",
  "styles": {
    "heading": {
      "fontFamily": "heading",
      "color": "grey-90",
      "lineHeight": "heading",
      "fontWeight": "heading",
      "m": 0
    },
    "h1": {
      "variant": "styles.heading",
      "fontSize": ["xxl", "xxxl"],
      "my": "md"
    },
    "h2": {
      "variant": "styles.heading",
      "fontSize": ["xl", "xxl"],
      "my": "md"
    },
    "h3": {
      "variant": "styles.heading",
      "fontSize": ["lg", "xl"],
      "my": "sm"
    },
    "h4": {
      "variant": "styles.heading",
      "fontSize": ["md", "lg"],
      "my": "sm"
    },
    "h5": {
      "variant": "styles.heading",
      "fontSize": ["sm", "md"],
      "my": "sm"
    },
    "h6": {
      "variant": "styles.heading",
      "fontSize": "sm",
      "my": "sm"
    },
    "p": {
      "mb": "sm",
      "lineHeight": "base"
    },
    "root": {
      "fontFamily": "base",
      "fontSize": "md",
      "fontWeight": "base",
      "lineHeight": "base",
      "color": "grey-80",
      "-webkit-font-smoothing": "antialiased",
      "-moz-osx-font-smoothing": "grayscale"
    },
    "a": {
      "color": "brand",
      "stroke": "currentColor",
      "textDecoration": "underline",
      "transition": "opacity 0.2s ease",
      ":hover": {
        "opacity": 0.8
      }
    },
    "blockquote": {
      "backgroundColor": "grey-10",
      "px": "md",
      "py": "sm",
      "mx": 0,
      "borderLeft": "none",
      "p": {
        "pl": "sm",
        "py": "xs",
        "my": 0,
        "color": "experimental-text",
        "borderLeft": "2px solid",
        "borderLeftColor": "grey-80",
        "fontWeight": "bold",
        "fontStyle": "normal"
      }
    },
    "table": {
      "borderSpacing": 0,
      "width": "100%",
      "borderStyle": "solid",
      "borderColor": "grey-20",
      "borderWidth": "1px",
      "borderRadius": "3px",
      "mb": "base"
    },
    "th": {
      "m": 0,
      "p": "16px",
      "fontSize": "sm",
      "textAlign": "left",
      "color": "experimental-text"
    },
    "td": {
      "m": 0,
      "p": "16px",
      "fontSize": "xs"
    },
    "tr": {
      ":nth-of-type(even)": {
        "backgroundColor": "grey-05"
      }
    }
  },
  "----------------------------- Variants": "-----------------------------",
  "elements": {
    "author-profile": {
      "base": {
        "details": {
          "mt": "md",
          "display": "flex",
          "px": 0,
          "a": { "mx": "sm" }
        },
        "bio": {
          "mt": "xs"
        },
        "name": {
          "mb": "md",
          "a": {
            "display": "flex",
            "flexDirection": "column"
          }
        },
        "role": {
          "variant": "styles.h6",
          "color": "brand",
          "mt": "xs",
          "mb": 0
        }
      },

      "variants": {
        "left": {
          "main": {
            "variant": "elements.author-profile.base",
            "a:first-child": { "ml": 0 }
          },
          "bio": {
            "variant": "elements.author-profile.base.bio",
            "pt": "md"
          },
          "image": {
            "position": "absolute"
          },
          "details": {
            "variant": "elements.author-profile.base.details"
          },
          "name": {
            "variant": "elements.author-profile.base.name",
            "ml": "xxxl",
            "pl": "xs"
          },
          "role": {
            "variant": "elements.author-profile.base.role"
          }
        },
        "centered": {
          "main": {
            "variant": "elements.author-profile.base",
            "display": "flex",
            "flexDirection": "column",
            "alignItems": "center"
          },
          "bio": {
            "variant": "elements.author-profile.base.bio",
            "textAlign": "center"
          },
          "image": {
            "alignSelf": "center",
            "mx": 0
          },
          "content": {
            "display": "flex",
            "flexDirection": "column",
            "alignItems": "center"
          },
          "details": {
            "variant": "elements.author-profile.base.details",
            "justifyContent": "center"
          },
          "name": {
            "variant": "elements.author-profile.base.name",
            "textAlign": "center"
          },
          "role": {
            "variant": "elements.author-profile.base.role"
          }
        }
      }
    },
    "logo-collection": {
      "base": {
        "inner": {
          "ml": ["xs", "xs", "0px"],
          "mr": ["xs", "xs", "xs"],
          "flex": "1 1 0px",
          "img": {
            "maxWidth": "100px"
          }
        }
      }
    },
    "author": {
      "imageLink": {
        "borderRadius": "50%",
        "position": "relative",
        "overflow": "hidden"
      },
      "image": {
        "position": "absolute",
        "top": 0,
        "bottom": 0,
        "right": 0,
        "left": "50%",
        "height": "100%",
        "maxWidth": "auto",
        "transform": "translateX(-50%)"
      },
      "link": {
        "fontWeight": "base"
      },
      "title": {
        "a": {
          "fontWeight": "bold",
          "color": "uswitch-navy",
          "fontSize": ["md", "xl"]
        }
      },
      "subtitle": {
        "fontSize": ["xs", "md"],
        "lineHeight": ["base", "heading"],
        "a": {
          "fontWeight": "bold",
          "color": "grey-60"
        }
      },
      "condensed": {
        "title": {
          "a": {
            "variant": "elements.author.title.a",
            "fontSize": ["md", "md"]
          }
        },
        "subtitle": {
          "paddingBottom": 0,
          "a": {
            "variant": "elements.author.subtitle.a",
            "fontSize": ["xs", "xs"]
          }
        },
        "imageLink": {
          "width": [40, 40],
          "height": [40, 40]
        },
        "base": {
          "a": {
            "variant": "elements.author.base.a"
          },
          "h5": {
            "marginBottom": [0, 0]
          }
        }
      }
    },
    "sponsored-by-tag": {
      "base": {
        "wrapper": {
          "display": "flex",
          "alignItems": "center",
          "justifyContent": "space-between",
          "paddingY": 4
        },
        "text": {
          "color": "grey-60",
          "fontSize": "xs",
          "fontFamily": "base",
          "fontWeight": "base",
          "fontStyle": "normal"
        },
        "image": {
          "margin-top": [null, "10px"]
        }
      },
      "pros": {
        "color": "green"
      },
      "cons": {
        "color": "red"
      },
      "pros-and-cons": {
        "title": {
          "color": "grey-60",
          "fontSize": "md",
          "fontWeight": "bold",
          "lineHeight": "heading"
        },
        "base": {
          "paddingY": "md",
          "paddingX": ["sm", "md"],
          "backgroundColor": "grey-05",
          "li": {
            "paddingLeft": 0,
            "fontWeight": "bold",
            "marginBottom": "xs",
            "lineHeight": "heading",
            "fontSize": "md",
            "display": "flex",
            "alignItems": "center",
            "::before": {
              "display": "none"
            }
          },
          ":last-child": {
            "marginBottom": "0"
          }
        },
        "icon": {
          "display": "inline",
          "marginRight": "xxs"
        }
      },
      "variants": {
        "pros": {
          "title": {
            "variant": "elements.list.pros-and-cons.title"
          },
          "base": {
            "variant": "elements.list.pros-and-cons.base",
            "svg": {
              "variant": "elements.list.pros-and-cons.icon"
            }
          }
        },
        "cons": {
          "title": {
            "variant": "elements.list.pros-and-cons.title"
          },
          "base": {
            "variant": "elements.list.pros-and-cons.base",
            "svg": {
              "variant": "elements.list.pros-and-cons.icon"
            }
          }
        }
      }
    },
    "input": {
      "base": {
        "fontFamily": "base",
        "fontSize": "base",
        "lineHeight": 1.33,
        "color": "text",
        "borderRadius": "4px",
        "appearance": "none",
        "border": "none",
        "background": "none",
        "boxSizing": "border-box",
        "display": "flex",
        "flex": 1,
        "position": "relative",
        "verticalAlign": "middle",
        "width": "100%",
        "&:focus": {
          "outline": "none"
        },
        "&:disabled": {
          "outline": "none",
          "border": "none",
          "color": "battleshipGrey",
          "backgroundColor": "veryLightGrey"
        }
      },
      "default": {
        "color": "grey-90"
      },
      "error": {
        "color": "tomato"
      },
      "focus": {
        "color": "grey-90"
      },
      "formInput": {
        "boxShadow": "formInput"
      },
      "frozen": {
        "base": {
          "backgroundColor": "frozen-bg",
          "borderRadius": "3px"
        },
        "button": {
          "background": "none",
          "border": "none",
          "height": "28px",
          "padding": "0 24px",
          "width": "69px",
          "cursor": "pointer",
          ":focus": {
            "outline": "2px solid",
            "outlineColor": "#008fe9"
          },
          "&::-moz-focus-inner": {
            "border": 0
          }
        },
        "hidden": {
          "display": "none"
        }
      },
      "wrapper": {
        "backgroundColor": "white",
        "boxSizing": "border-box",
        "display": "flex",
        "position": "relative",
        "borderRadius": "3px"
      },
      "affix": {
        "base": {
          "fontFamily": "base",
          "fontSize": "base",
          "lineHeight": "base",
          "textAlign": "center",
          "boxSizing": "border-box",
          "alignItems": "center",
          "display": "flex",
          "color": "text"
        },
        "prefix": {
          "variant": "elements.input.affix.base",
          "borderRightStyle": "solid",
          "borderRightWidth": 1
        },
        "suffix": {
          "variant": "elements.input.affix.base",
          "borderLeftStyle": "solid",
          "borderLeftWidth": 1
        }
      }
    },
    "input-alert": {
      "fontFamily": "base",
      "fontSize": "base",
      "lineHeight": 1.38,
      "color": "white",
      "padding": 8,
      "position": "relative",
      "borderRadius": "sm",
      "notificationColor": "#65717B",
      "alertColor": "tomato",
      "before": {
        "position": "absolute",
        "content": "''",
        "width": 0,
        "height": 0,
        "borderBottomStyle": "solid"
      }
    },
    "simple-input": {
      "base": {
        "fontFamily": "base",
        "fontSize": "base",
        "lineHeight": 1.33,
        "color": "text",
        "appearance": "none",
        "border": "none",
        "background": "none",
        "boxSizing": "border-box",
        "display": "flex",
        "flex": 1,
        "position": "relative",
        "verticalAlign": "middle",
        "width": "100%",
        "padding": "sm",
        "&:-webkit-autofill": {
          "padding": "15px",
          "margin": "1px"
        },
        "&::-webkit-inner-spin-button, -webkit-outer-spin-button": {
          "-webkit-appearance": "none",
          "margin": 0
        },
        "&:focus": {
          "outline": "none"
        },
        "&:disabled": {
          "outline": "none"
        }
      },
      "wrapper": {
        "borderRadius": "3px",
        "border": "solid 1px",
        "display": "flex",
        "position": "relative",
        "boxShadow": "formInput",
        "borderColor": "grey-90",
        "backgroundColor": "white",
        "&:disabled": {
          "outline": "none",
          "border": "none",
          "color": "battleshipGrey",
          "backgroundColor": "veryLightGrey",
          "span": {
            "color": "battleshipGrey"
          }
        }
      },
      "affix": {
        "base": {
          "alignItems": "center",
          "display": "flex",
          "color": "text",
          "my": "xs",
          "px": "12px"
        },
        "prefix": {
          "variant": "elements.simple-input.affix.base",
          "borderRightStyle": "solid",
          "borderRightWidth": 1
        },
        "suffix": {
          "variant": "elements.simple-input.affix.base",
          "borderLeftStyle": "solid",
          "borderLeftWidth": 1
        }
      },
      "inputWidth": {
        "small": 64
      },
      "variants": {
        "error": {
          "variant": "elements.simple-input.wrapper",
          "borderColor": "tomato",
          "boxShadow": "errorInput"
        },
        "focus": {
          "variant": "elements.simple-input.wrapper",
          "outline": "none",
          "borderColor": "grey-90",
          "boxShadow": "focusInput",
          "span": {
            "color": "grey-90"
          }
        }
      }
    },
    "toggle-switch": {
      "base": {
        "base": {
          "width": 70,
          "height": 30,
          "switch": {
            "width": 28,
            "height": 28
          }
        },
        "checked": {
          "variant": "elements.toggle-switch.base.base",
          "backgroundColor": "primary",
          "borderColor": "primary",
          "switch": {
            "variant": "elements.toggle-switch.base.base.switch",
            "backgroundColor": "white",
            "borderColor": "primary"
          }
        },
        "unchecked": {
          "variant": "elements.toggle-switch.base.base",
          "backgroundColor": "grey-20",
          "borderColor": "grey-20",
          "switch": {
            "variant": "elements.toggle-switch.base.base.switch",
            "backgroundColor": "white",
            "borderColor": "grey-20"
          }
        }
      },
      "variants": {
        "compact": {
          "checked": {
            "switch": {
              "variant": "elements.toggle-switch.base.base.switch",
              "backgroundColor": "primary",
              "borderColor": "primary"
            }
          },
          "unchecked": {
            "switch": {
              "variant": "elements.toggle-switch.base.base.switch",
              "backgroundColor": "grey-20",
              "borderColor": "grey-20"
            }
          }
        }
      }
    },
    "breadcrumbs": {
      "base": {
        "main": {
          "color": "grey-100"
        },
        "homeIcon": {
          "color": "grey-100"
        },
        "title": {
          "color": "grey-100"
        }
      },
      "variants": {
        "light": {
          "main": {
            "color": "grey-0"
          },
          "mobileLink": {
            "color": "grey-0"
          },
          "homeIcon": {
            "color": "grey-0"
          },
          "title": {
            "color": "grey-0"
          }
        }
      }
    },
    "category": {
      "main": {
        "bg": "primary",
        "mb": "md"
      },
      "heading": {
        "color": "grey-0"
      },
      "text": {
        "color": "grey-0"
      },
      "breadcrumbs": {
        "variant": "light"
      }
    },
    "call-out": {
      "main": {
        "backgroundColor": "grey-10",
        "px": ["sm", "md"]
      },
      "icon": {
        "backgroundColor": "grey-80"
      }
    },
    "cta": {
      "base": {
        "main": {
          "backgroundColor": "grey-10"
        },
        "title": {
          "color": "experimental-text",
          "fontWeight": "bold"
        },
        "text": {
          "mb": [null, 0]
        },
        "button": {
          "ml": [null, "md"]
        }
      },
      "variants": {
        "hero": {
          "main": {
            "backgroundColor": "none",
            "color": "white"
          },
          "title": {
            "variant": "elements.cta.base.title",
            "color": "inherit"
          },
          "text": {
            "variant": "elements.cta.base.text"
          },
          "button": {
            "variant": "elements.cta.base.button"
          }
        },
        "hero-white-bg": {
          "main": {
            "backgroundColor": "white"
          },
          "title": {
            "variant": "elements.cta.base.title"
          },
          "text": {
            "variant": "elements.cta.base.text"
          },
          "button": {
            "variant": "elements.cta.base.button"
          }
        }
      }
    },
    "buttons": {
      "base": {
        "px": "sm",
        "py": "sm",
        "fontWeight": "bold",
        "border": "none",
        "fontFamily": "heading",
        "borderRadius": "4px",
        ":disabled": {
          "opacity": "0.3",
          "cursor": "not-allowed"
        },
        "btnSize": {
          "large": {
            "px": "xl",
            "py": "base",
            "fontSize": "md"
          },
          "small": {
            "px": 40,
            "py": 13,
            "fontSize": "sm"
          }
        }
      },
      "variants": {
        "primary": {
          "variant": "elements.buttons.base",
          "color": "white",
          "backgroundColor": "primary",
          ":hover:not(:disabled)": {
            "backgroundColor": "primary--dark"
          },
          ":focus": {
            "backgroundColor": "primary--dark"
          },
          ":active": {
            "backgroundColor": "primary--dark"
          }
        },
        "secondary": {
          "variant": "elements.buttons.base",
          "color": "grey-70",
          "border": "#E0E1E4 1px solid",
          "backgroundColor": "button-secondary",
          ":hover:not(:disabled)": {
            "backgroundColor": "button-secondary--hover"
          },
          ":focus": {
            "backgroundColor": "button-secondary--hover"
          },
          ":active": {
            "backgroundColor": "button-secondary--hover"
          }
        },
        "icon": {
          "display": "flex",
          "flexDirection": ["column", "row", "row"],
          "button": {
            "variant": "elements.buttons.variants.secondary",
            "mb": ["sm", 0],
            "maxWidth": ["100%", "218px"],
            "boxShadow": "0px 2px 3px rgba(24, 24, 61, 0.06), 0px 5px 5px rgba(24, 24, 61, 0.06), 0px 8px 7px rgba(24, 24, 61, 0.06)",
            "span": {
              "display": "flex",
              "flexDirection": ["row", "column"],
              "justifyContent": ["flex-start", "center"],
              "alignItems": "center",
              "textAlign": "center",
              "fontWeight": "heading",
              "minHeight": [55, "auto"]
            },
            "img": {
              "maxHeight": ["inherit", 45],
              "maxWidth": [50, "inherit"],
              "mr": ["md", 0]
            }
          }
        }
      }
    },
    "list": {
      "bullet": {
        "color": "grey-80"
      },
      "numeric": {
        "color": "grey-80",
        "::before": {
          "content": "\" - \" counter(li)",
          "left": "-2em",
          "width": "2.5em"
        }
      },
      "pros": {
        "color": "green",
        "glyph": "tick"
      },
      "cons": {
        "color": "red",
        "glyph": "cross"
