  "fonts": {
    "base": "Roboto, -apple-system, system-ui, system-ui, Arial, sans-serif",
    "body": "Roboto, -apple-system, system-ui, system-ui, Arial, sans-serif",
    "heading": "sofia-pro, -apple-system, sans-serif",
    "monospace": "Menlo, monospace",
    "link": "Roboto, -apple-system, system-ui, system-ui, Arial, sans-serif"
