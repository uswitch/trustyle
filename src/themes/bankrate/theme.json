    "a": {
      "color": "brand",
      "stroke": "currentColor",
      "textDecoration": "underline",
      "transition": "opacity 0.2s ease",
      ":hover": {
        "opacity": 0.8
      }
    },
    "blockquote": {
      "backgroundColor": "grey-10",
      "px": "md",
      "py": "sm",
      "mx": 0,
      "borderLeft": "none",
      "p": {
        "pl": "sm",
        "py": "xs",
        "my": 0,
        "color": "experimental-text",
        "borderLeft": "2px solid",
        "borderLeftColor": "grey-80",
        "fontWeight": "bold",
        "fontStyle": "normal"
      }
    },
    "table": {
      "borderSpacing": 0,
      "width": "100%",
      "borderStyle": "solid",
      "borderColor": "grey-20",
      "borderWidth": "1px",
      "borderRadius": "3px",
      "mb": "base"
    },
    "th": {
      "m": 0,
      "p": "16px",
      "fontSize": "sm",
      "textAlign": "left",
      "color": "experimental-text"
    },
    "td": {
      "m": 0,
      "p": "16px",
      "fontSize": "xs"
    },
    "tr": {
      ":nth-of-type(even)": {
        "backgroundColor": "grey-05"
      }
    }
  },
