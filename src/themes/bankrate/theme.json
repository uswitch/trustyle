    "brand": "#0157FF",
    "text": "#4c4c4c",
    "grey-0": "#FFFFFF",
    "grey-05": "#F7F7F7",
    "grey-10": "#F2F2F7",
    "grey-20": "#DFDFE1",
    "grey-30": "#C2C4C8",
    "grey-40": "#ADAFB3",
    "grey-50": "#A0A2A6",
    "grey-60": "#6D6F73",
    "grey-70": "#47494F",
    "grey-80": "#24262A",
    "grey-90": "#141424",
    "grey-100": "#050505",
