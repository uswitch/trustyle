{
  "name": "saveOnEnergy",

  "useLocalStorage": false,
  "useCustomProperties": false,
  "useBodyStyles": false,

  "breakpoints": ["768px", "1280px"],

  "fonts": {
    "base": "Montserrat, sans-serif",
    "link": "Montserrat, sans-serif",
    "body": "Montserrat, sans-serif",
    "heading": "Montserrat, sans-serif",
    "monospace": "Menlo, monospace"
  },

  "lineHeights": {
    "base": "26px",
    "sm": "16px",
    "md": "26px",
    "lg": "48px",
    "xl": "58px"
  },

  "fontSizes": {
    "base": 16,
    "xxs": 12,
    "xs": 14,
    "sm": 20,
    "md": 24,
    "lg": 32,
    "xl": 42,
    "xxl": 52
  },

  "fontWeights": {
    "base": 400,
    "light": 300,
    "bold": 600
  },

  "space": {
    "outer": 64,
    "gutter": 15,
    "base": 16,
    "xxs": 8,
    "xs": 12,
    "sm": 16,
    "md": 24,
    "lg": 36,
    "xl": 56,
    "xxl": 64
  },

  "sizes": {
    "grid": {
      "verticalGutter": "md",
      "gutter": ["gutter", "xxl", "sm"],
      "container": {
        "maxWidth": [992, 992, 1170]
      },

      "lg-docs": "// 84 + 16 gutter up to 12 columns",
      "lg": {
        "1": 83.75,
        "2": 182.5,
        "3": 281.25,
        "4": 380,
        "5": 478.75,
        "6": 577.5,
        "7": 676.25,
        "8": 775,
        "9": 873.75,
        "10": 972.5,
        "11": 1071.25,
        "12": 1170
      },
      "md-docs": "// 64 + 16 gutter up to 12 columns",
      "md": {
        "1": 47.75,
        "2": 110.5,
        "3": 173.25,
        "4": 236,
        "5": 298.75,
        "6": 361.5,
        "7": 424.25,
        "8": 487,
        "9": 549.75,
        "10": 612.5,
        "11": 675.25,
        "12": 738
      },
      "sm-docs": "// 58 + 16 gutter up to 12 columns",
      "sm": {
        "1": 58,
        "2": 132,
        "3": 206,
        "4": 280
      }
    },
    "hero": { "width": 1280 },
    "label": { "width": 280 },
    "button": { "height": 58 },
    "footer": { "width": 576 },
    "card": {
      "maxWidth": 400,
      "content": { "maxWidth": 490 },
      "image": {
        "maxHeight": {
          "mobile": 145,
          "tablet": 170,
          "desktop": 170
        }
      },
      "featured": {
        "image": {
          "maxHeight": {
            "mobile": 145,
            "tablet": 205,
            "desktop": 295
          }
        }
      }
    },
    "click-out-form": { "elements": { "height": 50 } }
  },

  "colors": {
    "primary": "#446db5",
    "secondary": "#00aa4e",
    "heading": "#443160",
    "link": "#009fbb",
    "brand": "#443160",
    "accent-1": "#00aa4e",
    "accent-2": "#6abd45",
    "accent-3": "#00b9d9",
    "text": "#000000",
    "dark-1": "#000000",
    "dark-2": "#3d3d3d",
    "light-1": "#ffffff",
    "light-2": "#f9f9f9",
    "light-3": "#e6e6e6",
    "hero": "rgba(212, 242, 255, 0.8)",
    "hero-solid": "D4F2F1",
    "frozen-bg": "#F6F6F6",
    "battleshipGrey": "#65717B",
    "veryLightGrey": "#E9EBED",
    "uswitch-navy": "#141424",
    "grey-60": "#6D6F73",
    "error": "#DB1818",
    "dark-grey-03": "rgba(104,104,135,0.3)",
    "medium-grey": "#545454",
    "table-row": "rgba(0, 185, 217, 0.05)"
  },

  "radii": {
    "sm": 3,
    "lg": 16,
    "xl": 30,
    "form": 8
  },

  "srOnly": {
    "position": "absolute",
    "width": 1,
    "height": 1,
    "padding": 0,
    "margin": -1,
    "overflow": "hidden",
    "clip": "rect(0, 0, 0, 0)",
    "border": 0
  },

  "styles": {
    "root": {
      "fontFamily": "base",
      "fontSize": "base",
      "fontWeight": "base",
      "lineHeight": "base",
      "color": "text"
    },
    "p": {
      "color": "text"
    },
    "h1": {
      "lineHeight": ["lg", "lg", "xl"],
      "color": "heading",
      "fontFamily": "heading",
      "fontSize": ["lg", "lg", "xxl"],
      "fontWeight": "bold",
      "margin": 0,
      "mb": "sm"
    },
    "h2": {
      "lineHeight": "lg",
      "color": "heading",
      "fontFamily": "heading",
      "fontSize": ["lg", "lg", "xl"],
      "fontWeight": "light",
      "mb": "base"
    },
    "h3": {
      "margin": 0,
      "fontFamily": "heading",
      "fontSize": "md",
      "fontWeight": "light",
      "color": "brand",
      "mb": "xxs",
      "p": 0
    },
    "h4": {
      "color": "heading",
      "fontFamily": "heading",
      "fontSize": "md",
      "fontWeight": "light",
      "pb": "base"
    },
    "a": {
      "color": "link",
      "stroke": "link",
      "transition": "all 0.1s ease",
      ":visited": {
        "color": "link",
        "stroke": "link"
      },
      ":hover": {
        "color": "brand",
        "stroke": "brand"
      },
      "> *": {
        "transition": "all 0.1s ease"
      },
      "dark": {
        "color": "light-1",
        "stroke": "light-1",
        ":visited": { "color": "light-1", "stroke": "light-1" },
        ":hover": { "color": "accent-1", "stroke": "accent-1" }
      }
    },
    "table": {
      "borderSpacing": 0,
      "width": "100%",
      "marginBottom": "base"
    },
    "th": {
      "margin": 0,
      "p": "xxs",
      "fontSize": "base",
      "textAlign": "left",
      "color": "primary",
      "fontWeight": "md",
      "lineHeight": "base",
      "vertical": {
        "whiteSpace": ["wrap", "nowrap"]
      }
    },
    "td": {
      "margin": 0,
      "padding": "xxs",
      "fontSize": "base",
      "color": "medium-grey",
      "fontWeight": "base",
      "lineHeight": "base"
    },
    "tr": {
      ":nth-of-type(even)": {
        "backgroundColor": "table-row"
      }
    }
  },

  "----------------------------- Variants": "-----------------------------",

  "elements": {
    "author-profile": {
      "base": {
        "main": {
          "a:first-of-type": {
            "mr": 0
          },
          "span": {
            "fontFamily": "base",
            "display": "block"
          },
          "span:first-of-type": {
            "fontSize": "lg",
            "color": "black"
          },
          "span:nth-of-type(2)": {
            "fontSize": "md",
            "fontWeight": "normal",
            ":hover": {
              "color": "#009fbb"
            }
          },
          "fontFamily": "base",
          "fontSize": "lg",
          "flex-direction": "column",
          "display": "grid",
          "textAlign": "center",
          "justifyItems": "center",

          "name": {
            "display": "flex",
            "justifyContent": "center"
          },
          "div:first-of-type": {
            "pt": "md",
            "display": "grid",
            "justifyItems": "center"
          },
          "p": {
            "color": "medium-grey",
            "fontSize": "base"
          }
        },
        "details": {
          "margin-top": "-10px !important",
          "paddingTop": "0px !important",
          "padding-left": "0px !important",
          "display": "inline-flex !important",
          "a": {
            "px": "base",
            "color": "initial",
            "stroke": "initial"
          }
        }
      },
      "variants": {
        "left": {
          "main": {
            "span": {
              "fontFamily": "base",
              "display": "block",
              "position": "relative",
              "top": "-10px"
            },
            "span:first-of-type": {
              "color": "black",
              "fontSize": "lg"
            },
            "span:nth-of-type(2)": {
              "fontSize": "md",
              "fontWeight": "normal",
              ":hover": {
                "color": "#009fbb"
              }
            },
            "p": {
              "position": "relative",
              "left": " -100px",
              "color": "medium-grey",
              "fontSize": "base"
            }
          },
          "details": {
            "variant": "elements.author-profile.base.details",
            "position": "relative",
            "left": " -100px",
            "paddingTop": "md",
            "div": {
              "paddingLeft": "0px !important",
              "paddingRight": "base"
            },
            "a:first-of-type": {
              "paddingLeft": "0px !important"
            },
            "a": {
              "paddingRight": "sm",
              "color": "initial",
              "stroke": "initial"
            }
          }
        }
      }
    },
    "simple-input": {
      "base": {
        "fontFamily": "base",
        "fontSize": "base",
        "lineHeight": 1.33,
        "color": "text",
        "appearance": "none",
        "border": "none",
        "background": "none",
        "boxSizing": "border-box",
        "display": "flex",
        "flex": 1,
        "position": "relative",
        "verticalAlign": "middle",
        "width": "100%",
        "padding": "sm",
        "&:-webkit-autofill": {
          "padding": "15px",
          "margin": "1px"
        },
        "&::-webkit-inner-spin-button, -webkit-outer-spin-button": {
          "-webkit-appearance": "none",
          "margin": 0
        },
        "&:focus": {
          "outline": "none"
        },
        "&:disabled": {
          "outline": "none"
        }
      },
      "wrapper": {
        "borderRadius": "3px",
        "border": "solid 1px",
        "display": "flex",
        "position": "relative",
        "boxShadow": "formInput",
        "borderColor": "uswitch-navy",
        "backgroundColor": "white",
        "&:disabled": {
          "outline": "none",
          "border": "none",
          "color": "battleshipGrey",
          "backgroundColor": "veryLightGrey",
          "span": {
            "color": "battleshipGrey"
          }
        }
      },
      "affix": {
        "base": {
          "alignItems": "center",
          "display": "flex",
          "color": "text",
          "my": "xxs",
          "px": "xs"
        },
        "prefix": {
          "variant": "elements.simple-input.affix.base",
          "borderRightStyle": "solid",
          "borderRightWidth": 1
        },
        "suffix": {
          "variant": "elements.simple-input.affix.base",
          "borderLeftStyle": "solid",
          "borderLeftWidth": 1
        }
      },
      "variants": {
        "form": {
          "wrapper": {
            "border": "none"
          }
        },
        "error": {
          "variant": "elements.simple-input.wrapper",
          "borderColor": "error",
          "boxShadow": "errorInput"
        },
        "focus": {
          "variant": "elements.simple-input.wrapper",
          "outline": "none",
          "borderColor": "uswitch-navy",
          "boxShadow": "focusInput",
          "span": {
            "color": "uswitch-navy"
          }
        }
      }
    },
    "form": {
      "label": {
        "variant": "styles.h4",
        "color": "grey-80",
        "display": "block",
        ":not(:first-child)": {
          "mt": "sm"
        },
        "variants": {
          "calculator": {
            "variant": "styles.h6",
            "fontWeight": "bold",
            "color": "grey-80",
            "display": "block",
            "mb": "xs",
            ":not(:first-child)": {
              "mt": "xl"
            }
          }
        }
      },
      "button": {
        "my": "sm",
        "variants": {
          "calculator": {
            "mb": "sm",
            "mt": "lg"
          }
        }
      },
      "wrapper": {
        "base": {
          "pb": 0
        },
        "variants": {
          "calculator": {
            "pb": ["xs", 0]
          }
        }
      },
      "variants": {
        "panel": {
          "variant": "elements.form.base",
          "button": {
            "width": "100%",
            "mt": "xxs",
            "fontWeight": "base",
            "lineHeight": "md",
            "mb": 0
          },
          "label ~ div:first-of-type": {
            "borderColor": "light-3",
            "boxShadow": "none"
          },
          "label": {
            "fontSize": "base",
            "fontWeight": "base",
            "lineHeight": "md",
            "pb": "xxs"
          }
        }
      }
    },

    "rich-text-block": {
      "base": {
        "*:first-child": {
          "mt": 0
        }
      },
      "variants": {
        "panel-primary": {
          "bg": "white",
          "margin": "auto",
          "mr": ["auto", "lg"],
          "borderRadius": "form",
          "p": "md",
          "maxWidth": ["none", 445],
          "mt": ["md", null],
          ">div": {
            "height": "100%",
            "display": "inline-flex",
            "mb": "xxs",
            "justifyContent": "center",
            "boxSizing": "border-box",
            ":first-of-type": {
              "width": "50%",
              "borderRight": "1px solid",
              "borderRightColor": "light-3",
              "boxSizing": "border-box",
              "px": ["md", "lg"],
              "div": {
                "p": 0
              }
            },
            ":nth-of-type(2)": {
              "display": "none"
            },
            ":nth-of-type(3)": {
              "width": "50%"
            },
            ":last-of-type": {
              "&,div": {
                "width": "100%"
              }
            }
          },
          "form": {
            "variant": "elements.form.variants.panel"
          }
        },
        "center": {
          "display": "flex",
          "alignItems": "center",
          "justifyContent": "center",
          "height": "100%",
          "textAlign": ["center", "start"],
          "pt": ["lg", 0],
          "h1": {
            "mb": 0
          },
          "h2": {
            "my": 0,
            "fontSize": ["md", "lg", "lg"]
          }
        },
        "left": {
          "variant": "elements.rich-text-block.variants.center",
          "justifyContent": ["center", "flex-start"],
          "textAlign": ["center", "start"]
        }
      }
    },

    "input": {
      "box-sizing": "border-box",
      "height": 58
    },

    "button": {
      "height": 58
    },

    "buttons": {
      "variants": {
        "primary": {
          "color": "light-1",
          "border": "none",
          "borderRadius": "sm",
          "backgroundColor": "primary",
          "backgroundImage": "url(/uk/static/images/rarr.svg)",
          "backgroundRepeat": "no-repeat",
          "backgroundPosition": ["95% center", "90% center", "90% center"],
          "backgroundSize": ["auto", "auto", "13%"],
          "width": ["100%", "grid.md.3", "grid.lg.2"]
        },
        "secondary": {
          "color": "light-1",
          "backgroundColor": "secondary"
        }
      }
    },

    "hero": {
      "sm": {
        "py": 34
      },
      "md": {
        "py": 56
      },
      "lg": {
        "py": 82
      }
    },

    "link-with-chevron": {
      "marginLeft": 8,
      "textDecoration": "underline"
    },
    "author": {
      "base": {
        "fontSize": "base",
        "fontWeight": "light",
        "mb": "lg"
      },
      "imageLink": {
        "width": [48, 64],
        "height": [48, 64]
      },
      "text": {
        "fontSize": "inherit",
        "fontWeight": "inherit",
        "margin": 0,
        "mb": "md"
      },
      "title": {
        "fontSize": "sm",
        "fontWeight": "base",
        "mb": [0, 0, 0]
      },
      "subtitle": {
        "variant": "elements.author.text",
        "mb": 0
      },
      "link": {
        "variant": "elements.author.text",
        "mb": 0
      }
    }
  },

  "compounds": {
    "card": {
      "base": {
        "display": "flex",
        "flexDirection": ["column", "row"],
        "border": "1px solid",
        "borderColor": "light-3",
        "p": "xxs",
        "width": "100%",
        "boxSizing": "border-box",
        "maxWidth": "card.maxWidth",
        "image": {
          "width": "100%",
          "&,img": {
            "maxHeight": [
              "card.image.maxHeight.mobile",
              "card.image.maxHeight.tablet",
              "card.image.maxHeight.desktop"
            ],
            "height": "100%",
            "width": "100%"
          },
          "img": {
            "objectFit": "cover"
          }
        },
        "content": {
          "px": ["xxs", null],
          ">p": {
            "my": "xxs",
            "color": "medium-grey"
          }
        },
        "link": {
          "color": "text",
          "a,&": {
            "color": "link"
          },
          "&:last-of-type": {
            "pb": "sm"
          }
        },
        "h3": {
          "mt": "sm",
          "a": {
            "textDecoration": "none",
            "color": "heading",
            "&,:visited": {
              "color": "heading"
            },
            "fontSize": ["sm", "md"],
            "fontWeight": "base",
            "lineHeight": 1.3
          },
          ":hover": {
            "textDecoration": "underline"
          }
        }
      },
      "variants": {
        "vertical": {
          "variant": "compounds.card.base",
          "flexDirection": ["column", "column"],
          "content": {
            "variant": "compounds.card.base.content"
          },
          "link": {
            "variant": "compounds.card.base.link"
          },
          "image": {
            "variant": "compounds.card.base.image"
          },
          "h3": {
            "variant": "compounds.card.base.h3"
          },
          "superScript": {
            "variant": "compounds.card.base.superScript"
          },
          "headerChildren": {
            "variant": "compounds.card.base.headerChildren"
          }
        },
        "horizontal": {
          "variant": "compounds.card.base",
          "flexDirection": ["row", "row"],
          "alignItems": "start",
          "image": {
            "variant": "compounds.card.base.image",
            "width": 474,
            "mr": 32
          },

          "content": {
            "maxWidth": 490,
            "width": "100%",
            "wordBreak": "breakWord",
            "variant": "compounds.card.base.content"
          },
          "link": {
            "variant": "compounds.card.base.link"
          },
          "h3": {
            "variant": "compounds.card.base.h3"
          },
          "superScript": {
            "variant": "compounds.card.base.superScript"
          },
          "headerChildren": {
            "variant": "compounds.card.base.headerChildren"
          }
        },
        "responsive": {
          "variant": [
            "compounds.card.variants.vertical",
            "compounds.card.variants.horizontal"
          ],
          "image": {
            "variant": [
              "compounds.card.variants.vertical.image",
              "compounds.card.variants.horizontal.image"
            ]
          },
          "h3": {
            "variant": [
              "compounds.card.variants.vertical.h3",
              "compounds.card.variants.horizontal.h3"
            ]
          },

          "content": {
            "variant": [
              "compounds.card.variants.vertical.content",
              "compounds.card.variants.horizontal.content"
            ]
          },
          "link": {
            "variant": [
              "compounds.card.variants.vertical.link",
              "compounds.card.variants.horizontal.link"
            ]
          },
          "superScript": {
            "variant": [
              "compounds.card.variants.vertical.superScript",
              "compounds.card.variants.horizontal.superScript"
            ]
          },
          "headerChildren": {
            "variant": [
