      }
    },
    "click-out-form": {
      "flexDirection": ["column", "row", "row"],
      "width": "100%",
      "div:last-of-type": {
        "width": ["100%", "grid.md.5", "grid.lg.3"],
        "mr": [0, "sm"],
        "input": {
          "width": "100%",
          "height": "click-out-form.elements.height",
          "mb": 0
        }
      },
      "button": {
        "alignSelf": "flex-end",
        "width": ["100%", "grid.md.4", "grid.lg.2"],
        "mt": "sm"
