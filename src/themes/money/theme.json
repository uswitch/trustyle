    "radioish": {
      "base": {
        "&:checked + span": {
          "borderColor": "black",
          "boxShadow": "linkInset",
          "color": "black",
          "&::before": {
            "borderColor": "black"
          }
        },
        "&:focus + span": {
          "variant": "input.focus"
        }
      },
      "label": {
        "borderColor": "grey-40",
        "borderStyle": "solid",
        "borderWidth": "1px",
        "color": "grey-80",
        "fontWeight": "normal",
        "padding": "11px 12px 11px 0",
        "&:before": {
          "borderColor": "grey-30",
          "borderStyle": "solid",
          "borderWidth": "2px"
        }
      },
      "variants": {
        "normal": {
          "variant": "input.radioish.base"
        },
        "slim": {
          "variant": "input.radioish.base"
        }
      }
    },
    "radio": {
      "base": {
        "variant": "input.radioish.base",
        "&:checked + span": {
          "variant": "input.radioish.base.&:checked + span",
          "&::before": {
            "variant": "input.radioish.base.&:checked + span.&::before",
            "background": "black",
            "boxShadow": "inset 0 0 0 2px #fff"
          }
        }
      },
      "label": {
        "variant": "input.radioish.label",
        "&:before": {
          "variant": "input.radioish.label.&:before",
          "borderRadius": "50%"
        }
      }
    },
    "checkbox": {
      "base": {
        "variant": "input.radioish.base",
        "&:checked + span": {
          "variant": "input.radioish.base.&:checked + span",
          "&::before": {
            "variant": "input.radioish.base.&:checked + span.&::before",
            "backgroundImage": "url(\"data:image/svg+xml;charset=utf8,%3Csvg xmlns='http://www.w3.org/2000/svg' fill='%23FFFFFF' viewBox='-1 -2 14 12'%3E%3Cpath d='M9.603 1L11 2.507 4.681 9 1 4.898l1.453-1.446L4.74 5.999z' /%3E%3C/svg%3E\")",
            "backgroundSize": "16px 16px",
            "backgroundColor": "black",
            "borderColor": "black"
          }
        }
      },
      "label": {
        "variant": "input.radioish.label",
        "&:before": {
          "variant": "input.radioish.label.&:before",
          "borderRadius": "4px"
        }
      },
      "variants": {
        "normal": {
          "variant": "input.checkbox.base"
        },
        "slim": {
          "variant": "input.checkbox.base",
          "& + span": {
            "padding": "0px",
            "border": "none"
          },
          "&:checked + span": {
            "variant": "input.checkbox.base.&:checked + span",
            "boxShadow": "none",
            "&::before": {
              "variant": "input.checkbox.base.&:checked + span.&::before"
            }
          },
          "&:focus + span": {},
          "&:focus + span::before": {
            "variant": "input.focus"
          }
        }
      }
    },
    "tile": {
      "inputColor": "#008fe9",
      "borderColor": "#b0b8bf",
      "borderRadius": "3px",
      "borderStyle": "solid",
      "borderWidth": "1px",
      "color": "slate-grey",
      "fontWeight": "normal",
      "before": {
        "content": "' '",
        "borderColor": "#b0b8bf",
        "borderStyle": "solid",
        "borderWidth": "2px"
      }
    }
  },

